{
  "manifestVersion": 1,
  "id": "AgileManifestoDashboardWidget",
  "version": "1.0.36",
  "name": "Agile Manifesto",
  "publisher": "GregTrevellick",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "A dashboard widget to share the software development agile manifesto with your team",
  "icons": {
    "default": "app/Images/DefaultIcon.png"
  },
  "categories": [
    "Azure Boards "
  ],
  "tags": [
    "Agile",
    "Colaboration",
    "Manifesto",
    "Scrum",
    "widget"
  ],

  "links": {
    "home": {
      "uri": "https://marketplace.visualstudio.com/items?itemName=GregTrevellick.AgileManifestoDashboardWidget"
    },
    "repository": {
      "uri": "https://github.com/GregTrevellick/AzureDevOpsAgileManifestoWidget"
    },
    "issues": {
      "uri": "https://github.com/GregTrevellick/AzureDevOpsAgileManifestoWidget/issues"
    },
    "support": {
      "uri": "https://github.com/GregTrevellick/AzureDevOpsAgileManifestoWidget/issues"
    },
    "getstarted": {
      "uri": "https://agilemanifesto.org/"
    },
    "learn": {
      "uri": "https://agilemanifesto.org/"
    },
    "privacypolicy": {
      "uri": "https://gregtrevellick.github.io/Azure-DevOps-Widget-Privacy-Policy/"
    }
  },
  "screenshots": [
    {
      "path": "artefacts/Screenshot.png"
    },
    {
      "path": "artefacts/Screenshot2.png"
    }
  ],
  "branding": {
    "color": "rgb(1, 122, 215)",
    "theme": "dark"
  },

  "contributions": [
    {
      "id": "JSParamValue_AgileManifestoWidget",
      "type": "ms.vss-dashboards-web.widget",
      "targets": [
        "ms.vss-dashboards-web.widget-catalog",
        "GregTrevellick.AgileManifestoDashboardWidget.JSParamValue_AgileManifestoWidget_Configuration"
      ],
      "properties": {
        "name": "The Agile Manifesto",
        "description": "The agile manifesto statement for software development",
        "catalogIconUrl:": "app/Images/CatalogIcon.png",
        "previewImageUrl": "app/Images/DefaultIcon.png",
        "uri": "app/dashboard-widget.html",
        "supportedSizes": [
          {
            "rowSpan": 2,
            "columnSpan": 3
          },
          {
            "rowSpan": 3,
            "columnSpan": 3
          },
          {
            "rowSpan": 2,
            "columnSpan": 2
          },
          {
            "rowSpan": 3,
            "columnSpan": 2
          },
          {
            "rowSpan": 2,
            "columnSpan": 4
          },
          {
            "rowSpan": 3,
            "columnSpan": 4
          }
        ],
        "supportedScopes": [ "project_team" ]
      }
    },

    {
      "id": "JSParamValue_AgileManifestoWidget_Configuration",
      "type": "ms.vss-dashboards-web.widget-configuration",
      "targets": [
        "ms.vss-dashboards-web.widget-configuration"
      ],
      "properties": {
        "name": "The Agile Manifesto Configuration",
        "description": "Configures The Agile Manifesto",
        "uri": "app/configuration.html"
      }
    }
  ],

  "files": [
    {
      "path": "app/dashboard-widget.html",
      "addressable": true
    },
    {
      "path": "app/configuration.html",
      "addressable": true
    },
    {
      "path": "app/Content",
      "addressable": true
    },
    {
      "path": "app/Images",
      "addressable": true
    },
    {
      "path": "app/Scripts",
      "addressable": true
    }
  ],

  "content": {
    "details": {
      "path": "artefacts/Overview.md"
    },
    "license": {
      "path": "artefacts/Licence.md"
    }
  },

  "repository": {
    "type": "git",
    "uri": "https://github.com/GregTrevellick/AzureDevOpsAgileManifestoWidget"
  },

  "public": true

}
