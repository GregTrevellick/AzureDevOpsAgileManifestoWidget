{
  "manifestVersion": 1,
  "id": "CovidDashboardWidget",
  "version": "1.0.36",
  "name": "Covid19",
  "publisher": "GregTrevellick",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "A dashboard widget to share Covid19",
  "icons": {
    "default": "app/Images/DefaultIcon.png"
  },
  "categories": [
    "Azure Boards"
  ],
  "tags": [
    "Covid",
    "Covid19",
    "Covid19Virus",
    "Corona",
    "CoronaVirus",
    "widget"
  ],

  "links": {
    "home": {
      "uri": "https://www.nhs.uk/conditions/coronavirus-covid-19/"
    },
    "repository": {
      "uri": "https://github.com/GregTrevellick/AzureDevOpsCovid"
    }
  },
  "screenshots": [
    {
      "path": "artefacts/Screenshot.png"
    },
    {
      "path": "artefacts/Screenshot2.png"
    }
  ],
  "branding": {
    "color": "rgb(1, 122, 215)",
    "theme": "dark"
  },

  "contributions": [
    {
      "id": "JSParamValue_CovidWidget",
      "type": "ms.vss-dashboards-web.widget",
      "targets": [
        "ms.vss-dashboards-web.widget-catalog",
        "GregTrevellick.CovidDashboardWidget.JSParamValue_CovidWidget_Configuration"
      ],
      "properties": {
        "name": "Covid 19",
        "description": "Hopefully a Covid 19 life saver",
        "catalogIconUrl:": "app/Images/CatalogIcon.png",
        "previewImageUrl": "app/Images/DefaultIcon.png",
        "uri": "app/dashboard-widget.html",
        "supportedSizes": [
          {
            "rowSpan": 3,
            "columnSpan": 3
          }
        ],
        "supportedScopes": [ "project_team" ]
      }
    }
  ],

  "files": [
    {
      "path": "app/dashboard-widget.html",
      "addressable": true
    },
    {
      "path": "app/configuration.html",
      "addressable": true
    },
    {
      "path": "app/Content",
      "addressable": true
    },
    {
      "path": "app/Images",
      "addressable": true
    },
    {
      "path": "app/Scripts",
      "addressable": true
    }
  ],

  "content": {
    "details": {
      "path": "artefacts/Overview.md"
    },
    "license": {
      "path": "artefacts/Licence.md"
    }
  },

  "repository": {
    "type": "git",
    "uri": "https://github.com/GregTrevellick/AzureDevOpsCovidWidget"
  },

  "public": true

}
